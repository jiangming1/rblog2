<%- model_class = Smschongzhirecord -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:oldbi) %>:</strong></dt>
  <dd><%= @smschongzhirecord.oldbi %></dd>
  <dt><strong><%= model_class.human_attribute_name(:chongzhijine) %>:</strong></dt>
  <dd><%= @smschongzhirecord.chongzhijine %></dd>
  <dt><strong><%= model_class.human_attribute_name(:chongzhibi) %>:</strong></dt>
  <dd><%= @smschongzhirecord.chongzhibi %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dingdanhao) %>:</strong></dt>
  <dd><%= @smschongzhirecord.dingdanhao %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              smschongzhirecords_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_smschongzhirecord_path(@smschongzhirecord), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              smschongzhirecord_path(@smschongzhirecord),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
