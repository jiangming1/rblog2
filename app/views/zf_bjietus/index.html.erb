<%- model_class = ZfBjietu -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:biaotixingming) %></th>
      <th><%= model_class.human_attribute_name(:zhanghu) %></th>
      <th><%= model_class.human_attribute_name(:shouji) %></th>
      <th><%= model_class.human_attribute_name(:jiaoyihao) %></th>
      <th><%= model_class.human_attribute_name(:shijian) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @zf_bjietus.each do |zf_bjietu| %>
      <tr>
        <td><%= link_to zf_bjietu.id, zf_bjietu_path(zf_bjietu) %></td>
        <td><%= zf_bjietu.biaotixingming %></td>
        <td><%= zf_bjietu.zhanghu %></td>
        <td><%= zf_bjietu.shouji %></td>
        <td><%= zf_bjietu.jiaoyihao %></td>
        <td><%= zf_bjietu.shijian %></td>
        <td><%=l zf_bjietu.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_zf_bjietu_path(zf_bjietu), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      zf_bjietu_path(zf_bjietu),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_zf_bjietu_path,
            :class => 'btn btn-primary' %>
