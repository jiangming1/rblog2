<%- model_class = ZfBjietu -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:biaotixingming) %>:</strong></dt>
  <dd><%= @zf_bjietu.biaotixingming %></dd>
  <dt><strong><%= model_class.human_attribute_name(:zhanghu) %>:</strong></dt>
  <dd><%= @zf_bjietu.zhanghu %></dd>
  <dt><strong><%= model_class.human_attribute_name(:shouji) %>:</strong></dt>
  <dd><%= @zf_bjietu.shouji %></dd>
  <dt><strong><%= model_class.human_attribute_name(:jiaoyihao) %>:</strong></dt>
  <dd><%= @zf_bjietu.jiaoyihao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:shijian) %>:</strong></dt>
  <dd><%= @zf_bjietu.shijian %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              zf_bjietus_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_zf_bjietu_path(@zf_bjietu), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              zf_bjietu_path(@zf_bjietu),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
