<%- model_class = Smszfblog -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:zfbid) %>:</strong></dt>
  <dd><%= @smszfblog.zfbid %></dd>
  <dt><strong><%= model_class.human_attribute_name(:qian) %>:</strong></dt>
  <dd><%= @smszfblog.qian %></dd>
  <dt><strong><%= model_class.human_attribute_name(:used) %>:</strong></dt>
  <dd><%= @smszfblog.used %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              smszfblogs_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_smszfblog_path(@smszfblog), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              smszfblog_path(@smszfblog),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
