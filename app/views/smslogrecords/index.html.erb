<%- model_class = Smslogrecord -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:jieshouhaoma) %></th>
      <th><%= model_class.human_attribute_name(:neirong) %></th>
      <th><%= model_class.human_attribute_name(:xiangmu) %></th>
      <th><%= model_class.human_attribute_name(:huoqushijian) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @smslogrecords.each do |smslogrecord| %>
      <tr>
        <td><%= link_to smslogrecord.id, smslogrecord_path(smslogrecord) %></td>
        <td><%= smslogrecord.jieshouhaoma %></td>
        <td><%= smslogrecord.neirong %></td>
        <td><%= smslogrecord.xiangmu %></td>
        <td><%= smslogrecord.huoqushijian %></td>
        <td><%=l smslogrecord.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_smslogrecord_path(smslogrecord), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      smslogrecord_path(smslogrecord),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_smslogrecord_path,
            :class => 'btn btn-primary' %>
