<%- model_class = Smstaglist -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @smstaglist.name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:price) %>:</strong></dt>
  <dd><%= @smstaglist.price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:laiyuan) %>:</strong></dt>
  <dd><%= @smstaglist.laiyuan %></dd>
  <dt><strong><%= model_class.human_attribute_name(:isonline) %>:</strong></dt>
  <dd><%= @smstaglist.isonline %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              smstaglists_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_smstaglist_path(@smstaglist), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              smstaglist_path(@smstaglist),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
